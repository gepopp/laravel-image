{
    "name": "gepopp/image",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerhard Popp",
            "email": "gerhard@poppgerhard.at",
            "homepage": "https://poppgerhard.at"
        }
    ],
    "php": "8.0",
    "homepage": "https://github.com/gepopp/image",
    "keywords": ["Laravel", "Image"],
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Gepopp\\Image\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gepopp\\Image\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gepopp\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "Image": "Gepopp\\Image\\Facades\\Image"
            }
        }
    },
    "require": {
        "intervention/image": "^2.7"
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=config --provider=gepopp\\Image\\ImageServiceProvider"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=config --provider=gepopp\\Image\\ImageServiceProvider"
        ]
    }
}
